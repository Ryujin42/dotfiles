{
    "layer": "top",
    "position": "top",
    "modules-left": ["hyprland/workspaces"],
    "modules-center": ["clock"],
    "modules-right": ["cpu", "memory", "battery"],
    "hyprland/workspaces": {
        "format": "{icon}",
        "on-scoll-up": "hyprctl dispatch workspace e-1",
        "on-scoll-down": "hyprctl dispatch workspace e+1"
    },
    "clock": {
        "format": "{:%a %d-%m-%Y %H:%M}"
    },
    "battery": {
        "format": "{capacity}%"
    }
}

